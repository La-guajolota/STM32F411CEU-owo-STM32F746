#include "WS2812B.h"

void Set_LED(TIRA_t* regleta,uint8_t led_num,uint8_t R,uint8_t G,uint8_t B)
{
	regleta->LEDS[led_num].RGB[0] = R;
	regleta->LEDS[led_num].RGB[1] = G;
	regleta->LEDS[led_num].RGB[2] = B;
}
